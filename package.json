{
  "name": "react-webpack-static-boilerplate",
  "version": "1.0.0",
  "description": "React static isomorphic site. Compiled with webpack.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public/*",
    "static": "cp static/* public/",
    "compile": "NODE_ENV=production webpack -p --progress --stats --display-error-details --config webpack.dist.config.js",
    "build": "npm run clean && npm run static && npm run compile",
    "start": "npm run build && webpack-dev-server --config webpack.config.js"
  },
  "author": "Brian Egizi <me@brianegizi.com>",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "6.3.26",
    "babel-eslint": "5.0.0-beta6",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "6.3.19",
    "css-loader": "^0.23.1",
    "eslint": "1.10.3",
    "eslint-loader": "1.1.1",
    "eslint-plugin-react": "3.13.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "history": "1.17.0",
    "postcss-loader": "^0.8.0",
    "react": "0.14.3",
    "react-document-meta": "2.0.0",
    "react-dom": "0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-router": "1.0.3",
    "static-site-generator-webpack-plugin": "2.0.1",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.2",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0"
  },
  "dependencies": {}
}
